<h1>Listing in_app_types</h1>

<table>
  <thead>
    <tr>
      <th>Display name</th>
      <th>In app type</th>
      <th>Description</th>
      <th>Apple</th>
      <th>Apple bundle</th>
      <th>Apple type</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @in_app_types.each do |in_app_type| %>
      <tr>
        <td><%= in_app_type.display_name %></td>
        <td><%= in_app_type.in_app_type %></td>
        <td><%= in_app_type.description %></td>
        <td><%= in_app_type.apple_id %></td>
        <td><%= in_app_type.apple_bundle_id %></td>
        <td><%= in_app_type.apple_type %></td>
        <td><%= in_app_type.price %></td>
        <td><%= link_to 'Show', in_app_type %></td>
        <td><%= link_to 'Edit', edit_in_app_type_path(in_app_type) %></td>
        <td><%= link_to 'Destroy', in_app_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New In app type', new_in_app_type_path %>
